<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       xmlns:p="http://www.springframework.org/schema/p"
        >


	<bean id="forms.entryTypeIdentityPicker" class="fr.paris.lutece.plugins.forms.modules.identitypicker.service.entrytype.EntryTypeIdentityPicker" >
	<property name="anonymizationTypes">
            <list>
                <ref bean="genericattributes.entryIdAnonymizationType" />
                <ref bean="genericattributes.entryCodeAnonymizationType" />
                <ref bean="genericattributes.responseIdAnonymizationType" />
                <ref bean="forms.formIdAnonymizationType" />
                <ref bean="forms.stepIdAnonymizationType" />
                <ref bean="forms.questionTitleAnonymizationType" />
                <ref bean="genericattributes.randomGuidAnonymizationType" />
                <ref bean="genericattributes.randomNumberAnonymizationType" />
                <ref bean="genericattributes.defaultValueAnonymizationType" />
            </list>
        </property>
    </bean>
	
	
	<bean id="forms.entryTypeIdentityPickerDisplayService" class="fr.paris.lutece.plugins.forms.modules.identitypicker.web.entrytype.EntryTypeIdentityPickerDisplayService">
		<constructor-arg  type="java.lang.String" value="forms.entryTypeIdentityPicker" />
	</bean>
	
    <bean id="forms.entryTypeIdentityPickerService" class="fr.paris.lutece.plugins.forms.web.entrytype.EntryTypeDefaultDataService">
		<constructor-arg  type="java.lang.String" value="forms.entryTypeIdentityPicker" />
	</bean>
		
</beans>
